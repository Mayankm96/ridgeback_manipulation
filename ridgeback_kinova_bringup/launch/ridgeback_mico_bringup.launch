<launch>
  <arg name="serial_number"/>

  <include file="$(find kinova_bringup)/launch/kinova_robot.launch">
    <arg name="kinova_robotType" value="m1n6s300"/>
    <arg name="kinova_robotName" value="mico_arm"/>
    <arg name="kinova_robotSerial" value="$(arg serial_number)"/>
  </include>
</launch>
